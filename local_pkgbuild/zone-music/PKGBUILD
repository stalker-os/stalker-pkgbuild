# Maintainer: Gabriel Matthews <matthewriel63@gmail.com>

pkgname=zone-music
_destname1="/etc/skel/Music"
pkgver=1.0
pkgrel=1
pkgdesc="Images that are taken from The Zone."
arch=('x86_64')
url="https://github.com/zone-linux/zone-pkgbuild/local_pkgbuild/zone-music"
license=('MIT')
provides=("${pkgname}")
options=(!strip !emptydirs)

package() {
	install -dm755 ${pkgdir}${_destname1}
	cp -rf ../src/* ${pkgdir}${_destname1}
}
