# Maintainer: Gabriel Matthews <matthewriel63@gmail.com>

pkgname=zone-welcome
_destname1="/usr"
pkgver=1.0
pkgrel=1
pkgdesc="Autostart app for The Zone"
url="https://github.com/zone-linux/zone-pkgbuild/core_pkgbuild/zone-welcome"
arch=('any')
license=('MIT')
depends=('yad')
provides=("${pkgname}")
options=(!strip !emptydirs)

package() {
    install -dm755 ${pkgdir}${_destname1}
    cp -rf ..${_destname1}/* ${pkgdir}${_destname1}
}
